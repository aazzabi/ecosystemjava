<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Panier.LivraisonController">
    <stylesheets>
        <URL value="@livraison.css" />
    </stylesheets>
   <children>
      <Label layoutX="23.0" layoutY="14.0" text="Vos Livraisons" textFill="WHITE">
         <font>
            <Font name="BebasNeue" size="44.0" />
         </font>
      </Label>
      <Label layoutX="637.0" layoutY="38.0" text="Monsieur / Madame :" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="nom_prenom_u" layoutX="842.0" layoutY="43.0" text="Label" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TableView fx:id="table_livraison" layoutX="46.0" layoutY="82.0" prefHeight="200.0" prefWidth="881.0">
        <columns>
          <TableColumn fx:id="date" prefWidth="242.0" text="Date Livraison" />
          <TableColumn fx:id="etat" prefWidth="192.0" text="Etat" />
            <TableColumn fx:id="adresse" prefWidth="432.0" text="Adresse" />
        </columns>
      </TableView>
      <Button fx:id="annuler_livraison" layoutX="778.0" layoutY="325.0" mnemonicParsing="false" onAction="#Annuler_livraison" prefHeight="25.0" prefWidth="197.0" text="Annuler Livraison" />
