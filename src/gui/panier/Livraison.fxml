         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="46.0" layoutY="313.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nom &amp; Prénom du Livreur : ">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Label fx:id="np_livreur" layoutX="245.0" layoutY="293.0" text="Label" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="np_liv" layoutX="778.0" layoutY="361.0" mnemonicParsing="false" onAction="#LivreurName" text="Demander Nom &amp; Prénom Livreur" />
